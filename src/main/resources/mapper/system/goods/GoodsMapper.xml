<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wen.mall.system.goods.mapper.GoodsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wen.mall.system.goods.entity.Goods">
        <id column="uuid" property="uuid" />
        <result column="name" property="name" />
        <result column="main_pic_id" property="mainPicId" />
        <result column="post_info" property="postInfo" />
        <result column="specification" property="specification" />
        <result column="status" property="status" />
        <result column="retail_price" property="retailPrice" />
        <result column="wholesale_price" property="wholesalePrice" />
        <result column="update_time" property="updateTime" />
        <result column="catalog_id" property="catalogId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        uuid, name, main_pic_id, post_info, specification, status, retail_price, wholesale_price, update_time
    </sql>

</mapper>
