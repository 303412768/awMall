<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wen.mall.system.order.mapper.AddressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wen.mall.system.order.entity.Address">
        <id column="uuid" property="uuid" />
        <result column="name" property="name" />
        <result column="address" property="address" />
        <result column="tel" property="tel" />
        <result column="identity" property="identity" />
        <result column="wx_id" property="wxId" />
        <result column="order_id" property="orderId" />
        <result column="user_id" property="userId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        uuid, name, address, tel, identity, wx_id, order_id, user_id
    </sql>

</mapper>
