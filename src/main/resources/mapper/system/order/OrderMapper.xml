<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wen.mall.system.order.mapper.OrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wen.mall.system.order.entity.Order">
        <id column="uuid" property="uuid" />
        <result column="user_id" property="userId" />
        <result column="order_no" property="orderNo" />
        <result column="update_time" property="updateTime" />
        <result column="order_statues" property="orderStatues" />
        <result column="total" property="total" />
        <result column="express_company" property="expressCompany" />
        <result column="delivery_number" property="deliveryNumber" />
        <result column="delivery_fee" property="deliveryFee" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        uuid, user_id, order_no, update_time, order_statues, total, express_company, delivery_number, delivery_fee
    </sql>

</mapper>
